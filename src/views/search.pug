extends layouts/main.pug
include mixins/videoBlock

block content
  .search__wrapper
    .search__header
      h3 Searching by '#{searchingBy}'
    .search__videos
      if videos.length === 0
        h5.videos__not-found No Videos Found
      each item in videos
        +videoBlock({id: item.id,
                  title: item.title,
                  views: item.views,
                  description: item.description,
                  videoFile: item.videoFile})